{
  "scripts": {
    "// === DÉVELOPPEMENT LOCAL ===": "",
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:devclient": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "// === TESTS EXPO GO (GRATUIT) ===": "",
    "test:expo-go": "expo start --tunnel",
    "qr:generate": "expo start --tunnel --qr",
    "// === BUILDS DÉVELOPPEMENT ===": "",
    "build:dev": "eas build --profile development --platform all",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "// === BUILDS STAGING ===": "",
    "build:staging": "eas build --profile staging --platform all",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "// === BUILDS PREVIEW (TESTS) ===": "",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "// === BUILDS PRODUCTION ===": "",
    "build:prod": "eas build --profile production --platform all",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "// === DISTRIBUTION ===": "",
    "distribute:android": "eas build --profile staging --platform android --auto-submit",
    "distribute:ios:free": "eas build --profile development --platform ios",
    "distribute:ios:paid": "eas build --profile production --platform ios && eas submit --platform ios",
    "// === UPDATES OTA ===": "",
    "update:staging": "eas update --branch staging --message \"Staging update\"",
    "update:prod": "eas update --branch production --message \"Production update\"",
    "// === VALIDATION ET TESTS ===": "",
    "validate": "expo doctor",
    "test:build": "expo doctor && npm run build:preview",
    "credentials:check": "eas credentials",
    "// === WORKFLOWS COMPLETS ===": "",
    "deploy:testing": "npm run validate && npm run build:preview",
    "deploy:staging": "npm run validate && npm run build:staging && npm run update:staging",
    "deploy:prod": "npm run validate && npm run build:prod && npm run update:prod"
  },
  "dependencies": {
    "expo-updates": "~0.28.14",
    "react-native-safe-area-context": "^5.4.0",
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.2.10",
    "@reduxjs/toolkit": "^2.7.0",
    "@stomp/stompjs": "^7.1.1",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^19.1.3",
    "axios": "^1.9.0",
    "date-fns": "^2.30.0",
    "expo": "53.0.11",
    "expo-constants": "~17.1.6",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "formik": "^2.4.6",
    "js-cookie": "^3.0.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-event-listeners": "^1.0.7",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keychain": "^10.0.0",
    "react-native-paper": "^5.13.5",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0",
    "socket.io-client": "^4.8.1",
    "sockjs-client": "^1.6.1",
    "typescript": "~5.8.3",
    "yup": "^1.6.1",
    "expo-dev-client": "~5.2.0"
  },
